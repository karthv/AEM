<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
    xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    title="My Dialog"
    xtype="dialog">
    <items jcr:primaryType="cq:TabPanel">
        <tab1
            jcr:primaryType="cq:Panel"
            title="Tab 1">
            <items jcr:primaryType="cq:WidgetCollection">
                <checkbox1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                    fieldLabel="Checkbox 1"
                    name="./checkbox1"
                    value="true"
                    checked="{Boolean}true"
                    data-cq-dialog-checkbox-showhide=""
                    data-cq-dialog-checkbox-showhide-target-true="#textfield1"
                    data-cq-dialog-checkbox-showhide-target-false="#textfield2"/>
                <textfield1
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                    fieldLabel="Text Field 1"
                    name="./textfield1"
                    hidden="true"/>
                <textfield2
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                    fieldLabel="Text Field 2"
                    name="./textfield2"/>
            </items>
        </tab1>
        <tab2
            jcr:primaryType="cq:Panel"
            title="Tab 2">
            <items jcr:primaryType="cq:WidgetCollection">
                <checkbox2
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                    fieldLabel="Checkbox 2"
                    name="./checkbox2"
                    value="true"
                    checked="{Boolean}true"
                    data-cq-dialog-checkbox-showhide=""
                    data-cq-dialog-checkbox-showhide-target-true="#textfield3"
                    data-cq-dialog-checkbox-showhide-target-false="#textfield4"/>
                <textfield3
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                    fieldLabel="Text Field 3"
                    name="./textfield3"
                    hidden="true"/>
                <textfield4
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                    fieldLabel="Text Field 4"
                    name="./textfield4"/>
            </items>
        </tab2>
    </items>
</jcr:root>

